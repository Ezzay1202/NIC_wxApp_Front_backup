/* pages/home/home.wxss */
page{
  background-color: rgb(245, 245,245);
  --td-tab-nav-bg-color: rgb(245, 245,245);
}
.custom-tabs{
  background-color: blue;
}
t-search{
  width:80%;
  background-color: white;
}
.img {
  width: 100%;
  height: 100%;
}
.block {
  display: block;
  margin-bottom: 32rpx;
}
.image{
  padding: 1em 1em .8em;
  width: 60rpx;
  height: 60rpx;
}
.first-container{
  display:flex;
  flex-direction: column;
  background-color: #FFFFFF;
  margin: 0rpx 30rpx 26rpx 30rpx;
  box-shadow: 0 15rpx 10rpx -15rpx #efefef;
  border-radius: 20rpx;
  box-sizing: border-box;

}
.middle-container {
  height: 180rpx;
  display: flex;
  /* flex-direction: column; */
  align-items: center;
  box-shadow: 0 15rpx 10rpx -15rpx #efefef;
}
.hidden-container{
  margin-top: .5em;
  padding-bottom:1em ;
  border-top: 1px dashed black;
  width: 100%;
  box-shadow: 0 15rpx 10rpx -15rpx #efefef;
}
.view_2 {
  width: 75%;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.view_tupianwenzi {
  display: flex;
  flex-direction: column;
  align-items: center;
  float: left;
}
.view_tupianwenzi-four{
  width: 120rpx;
  margin-left: 25rpx;
  margin-right: 25rpx;
}
.view_tupianwenzi-five{
  width: 98rpx;
  margin-left: 20rpx;
  margin-right: 20rpx;
}

.image_tupian-four {
  display: flex;
  padding: 1em 1em .5em;
  width: 60rpx;
  height: 60rpx;
}
.image_tupian-five{
  display: flex;
  padding: .8em .8em .4em;
  width: 48rpx;
  height: 48rpx;
}

.text_1-four {
  font-size: 27rpx;
}
.text_1-five{
  font-size: 24rpx;
}
.outview{
  background-color: white;
  margin: 20rpx 30rpx 26rpx 30rpx;
  box-shadow: 0 15rpx 10rpx -15rpx #efefef;
  padding: 1em;
  border-radius: 5%;
  display: flex;
  flex-direction: column;
}
.flex-column{
  display:flex;
  flex-direction: column;
}
.myday{
  border-top: 1px dashed black;
  margin: .5em;
  max-height: 400rpx;
  overflow: scroll;
  display:flex;
  flex-direction: column;
}
.sticky{
  position:-webkit-sticky;
  position:sticky;
  top:0rpx;
  z-index: 3;
}
.date{

  text-align: center;
  margin: .3em;
  background-color: #efefef;
}
.mywork{
  height:120rpx;
  padding: .3em;
  margin:.5em;
  border-left:6rpx outset #009999;
  display: flex;
  flex-direction: column;
  box-shadow: 10rpx 10rpx 10rpx -15rpx #009999;
}
.mywork>text{
  flex: 1;
  font-size: 21rpx;
  margin-left:15rpx ;
  color: black ;
}
.text{
  font-size: 35rpx;
  font-weight: bold;
  margin-bottom: .3em;
}
.button-box{
  display: flex;
  margin:.5em .3em;
}
.button-box>button{
  flex:1;
  font-size: 25rpx;
  padding:.2em;
  margin-left:.3em;
  color: white;
  border-radius: 10rpx;
  background-color: #1f9792;
}
.worklist{
  height: 1200rpx;
}
.t-class{
  flex:1;
}
.t-class-content{
  margin-top: .2em !important; 
  max-height: 50rpx;
  overflow:scroll;
}
.t-class-title{
  font-size: 22rpx !important; 
  font-weight: 100;
}
.myM-ing{
  box-shadow: 10rpx 15rpx 10rpx -15rpx #009999;
  padding: 1em;
}
.m-ing{
  width: 85%;
  align-items: stretch;
  padding: 1em;
  border:.2em outset #135ba4 ;
  margin: .5em ;
  border-radius:5%;
  display:flex;
  flex-direction: column;
}
.n-ing{
  margin: .3em;
  padding:.2em;
  border-top: 1px dashed black;
}
.myM-history{
  padding: .5em;
  max-height: 400rpx;
  overflow:scroll;
}
.title{
  flex:1;
  font-size: 30rpx;
  max-height:3em;
  margin-bottom: .5em;
  overflow:scroll;
}
.swiper{
  overflow:scroll;
  height: 100%;
}
.item {
  display: inline-block;
  width: 44rpx;
  margin: 0 8rpx;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  background-color: #0052d9;
  border-radius: 2px;
}
.countdown{
  margin: 1em .5em;
}
van-count-down>text{
  font-size: 28rpx;
}

.m{
  width: 85%;
  align-items: stretch;
  padding: 1em;
  border:.1em solid rgb(236,242,254);
  background-color: #ecfbfe;
  margin: .5em ;
  border-radius:10%;
  display:flex;
  flex-direction: column;
}
 .title{
  flex:1;
  font-size: 30rpx;
  max-height:3em;
  margin-bottom: .5em;
  overflow:scroll;
}
.m .text{
  float: left;
  margin: .2em;
  max-height: 3em;
  overflow: scroll;
  border-bottom: 1px dashed #009999;
}
.m .text >text{
  color: white;
  background-color: #13a4a4;
  border-radius: .5em;
}
 .m text{
  float:inline-start;
  font-size: 20rpx;
  margin: .3em .8em .3em 0;
  padding: .5em;
} 
.text button{
  margin: .3em .8em .3em 0;
  font-size: 20rpx;
  color: white;
  background-color: #135ba4;
  border-radius: .5em;
}
.m .people{
  flex:1;
  float:left;
  margin: .2em;
  display: flex;
  max-height: 3rem;
  max-width: 80%;
  align-items:center;
  overflow:scroll;
}
.m .people >text{
  min-width: max-content;
  margin-right: .5em;
  text-align: center;
  flex:1;
  padding: .4em;
  font-size:20rpx;
  color:var(--people-color);
  border:1rpx solid var(--people-color);
  border-radius: .5em;
}
.m .people >text[data-id="1"]{
  --people-color:#13a45b
}
.m .people >text[data-id="2"]{
  --people-color:#135ba4
}
.m .people >text[data-id="4"]{
  --people-color:#a4a413
}
.m .people >text[data-id="5"]{
  --people-color:#a413a4
}

 .n{
  display: flex;
  box-sizing: border-box;
}
.score{
  box-sizing: border-box;
  width:20%;
  font-size:30rpx;
  padding: .3rem .3rem 0;
  color:#a63f53;
}